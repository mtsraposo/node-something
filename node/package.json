{
    "name": "node-something",
    "version": "1.0.0",
    "description": "",
    "main": "src",
    "engines": {
        "node": ">=20.0.0"
    },
    "scripts": {
        "build": "webpack --config webpack.config.js",
        "format": "prettier --write '*/**/*.{js,ts}'",
        "lint": "eslint '*/**/*.{js,ts}'",
        "migrate:up": "node src/db/migrator.js up",
        "migrate:down": "node src/db/migrator.js down",
        "new-seed": "npx sequelize-cli seed:generate --name",
        "prepare": "husky install",
        "seed": "npx sequelize-cli db:seed:all",
        "start": "node dist/main.js",
        "start-dev": "nodemon --exec babel-node src/app.js",
        "test": "bin/test"
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mtsraposo/node-something.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/mtsraposo/node-something/issues"
    },
    "homepage": "https://github.com/mtsraposo/node-something#readme",
    "dependencies": {
        "@kafkajs/confluent-schema-registry": "^3.3.0",
        "axios": "^1.4.0",
        "buffer": "^6.0.3",
        "dotenv": "^16.3.1",
        "events": "^3.3.0",
        "kafkajs": "^2.2.4",
        "pg": "^8.11.2",
        "pg-hstore": "^2.3.4",
        "qs": "^6.11.2",
        "redis": "^4.6.7",
        "sequelize": "^6.32.1",
        "umzug": "^3.3.1",
        "uuid": "^9.0.0",
        "ws": "^8.13.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.22.9",
        "@babel/core": "^7.22.9",
        "@babel/node": "^7.22.6",
        "@babel/preset-env": "^7.22.9",
        "@babel/register": "^7.22.5",
        "@types/jest": "^29.5.3",
        "babel-jest": "^29.6.1",
        "babel-loader": "^9.1.3",
        "coveralls": "^3.1.1",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.9.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.33.0",
        "husky": "^8.0.3",
        "jest": "^29.6.1",
        "lint-staged": "^13.2.3",
        "nodemon": "^3.0.1",
        "prettier": "3.0.0",
        "sequelize-cli": "^6.6.1",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1"
    }
